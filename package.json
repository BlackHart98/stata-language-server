{
  "name": "stata-language-server",
  "description": "Simple Stata Syntaxlint",
  "author": "HaiBo",
  "repository": "https://github.com/HankBO/stata-language-server",
  "license": "MIT",
  "version": "0.1.2",
  "publisher": "CHARLS",
  "engines": {
    "vscode": "^1.55.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onLanguage:stata"
  ],
  "contributes": {
    "commands": [
      {
        "command": "registerCompletions",
        "title": "Register completions"
      },
      {
        "command": "registerHover",
        "title": "Register Hover"
      },
      {
        "command": "registerDefinition",
        "title": "Register Definition"
      },
      {
        "command": "unregisterCompletions",
        "title": "Unregister completions"
      },
      {
        "command": "unregisterHover",
        "title": "Unregister Hover"
      },
      {
        "command": "unregisterDefinition",
        "title": "Unregister Definition"
      }
    ],
    "configuration": {
      "type": "object",
      "title": "Stata Server Configuration",
      "properties": {
        "stataServer.exampleConfiguration": {
          "scope": "resource",
          "type": "string",
          "default": "You can override this message."
        }
      }
    }
  },
  "main": "./client/out/extension",
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -b",
    "watch": "tsc -b -w"
  },
  "devDependencies": {
    "@types/node": "^14.14.41",
    "@types/vscode": "^1.55.0",
    "@typescript-eslint/eslint-plugin": "^4.22.0",
    "@typescript-eslint/parser": "^4.22.0",
    "eslint": "^7.23.0",
    "typescript": "^4.2.4"
  },
  "dependencies": {
    "vsce": "^1.96.1",
    "vscode-languageclient": "^7.0.0"
  }
}
